        <item name="colorPrimary">@color/fuxia</item>
        <item name="colorPrimaryVariant">@color/darkBiege</item>
        <item name="colorOnPrimary">@color/yellowPastel</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/limeLemon</item>
        <item name="colorSecondaryVariant">@color/sunBirst</item>
        <item name="colorOnSecondary">@color/deepestViolet</item>
